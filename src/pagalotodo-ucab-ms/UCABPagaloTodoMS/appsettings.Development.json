    "RequireSwagger": true,
    "RequireDatabase": true,
    "RequireAzureBus": false,
    "RequireAzureStorage": true,
    "RequireSendGrid": false,
    "RequireAuthorization": false,
    "UCABUrl": "http://localhost/",
